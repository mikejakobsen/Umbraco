@inherits UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
}


<div class="row">

    <div class="col-xs-12">



        @if (!Members.GetCurrentLoginStatus().IsLoggedIn)
        {
            @Html.Partial("Login")
            ;
        }
        else
        {
            @Html.Partial("Logout")
            ;
        }
    </div>

</div>